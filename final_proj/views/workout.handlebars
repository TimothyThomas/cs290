<h1>Welcome To Your Workout Tracker!</h1>

{{#if results}}
<table>
   <caption>Completed Exercises</caption>
   <thead>
     <tr>
         <th> Name 
         <th> Reps
         <th> Weight 
         <th> Date 
         <th> lbs
   <tbody>
     {{#each results}}
     <tr>
       <td> {{this.name}}
       <td> {{this.reps}}
       <td> {{this.weight}}
       <td> {{this.date}}
       <td> {{this.lbs}}
     {{/each}}
   </tbody>
</table>

{{else}}
<p>You haven't logged any exercises yet.</p>
{{/if}}
